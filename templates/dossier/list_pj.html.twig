<div class="card card-custom-2 m-0">
    <div class="card-header">
        <h3>LISTE DES PIECES JOINTES INSEREES</h3>
    </div>
</div>
<div class="table-responsive">
    <table id="listPjdossier" class="dataTable table table-striped">
        <thead>
        <th><span>N°</span></th>
        <th><span>Date d'ajout dossier</span></th>
        <th><span>Information PJ</span></th>
        <th><span>Lien d'accès</span></th>
        {% if app.user.hasRole("ROLE_SUPERADMIN") or app.user.hasRole("ROLE_ADMIN") %}
            <th><span>Action</span></th>
        {% else %}
            <th></th>
        {% endif %}
        </thead>
    </table>
</div>